{
  "version": 2,
  "buildCommand": "echo 'No build required for static site'",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    { "source": "/BRONCHURES/(.*)", "destination": "/BRONCHURES/$1" },
    { "source": "/public/(.*)", "destination": "/public/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
